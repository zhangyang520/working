package com.zhjy.iot.mobile.oa.entity;

import java.util.List;
/**
 * 
 * @类描述：   文件传阅条目详情实体
 * @创建人：HuangXianFeng 
 * @创建时间：2016-3-25 下午4:08:32  
 *
 */
public class FileTransactionDetail {
	
	private String title;
	private String content;
	private String sendPerson;
	private String sendTime;
	private List<AppendDoc> appendDocSet;
	private List<ReceivePeople> receivePeople;
	public FileTransactionDetail() {
		super();
	}
	public FileTransactionDetail(String title, String content,
			String sendPerson, String sendTime, List<AppendDoc> appendDocSet,
			List<ReceivePeople> receivePeople) {
		super();
		this.title = title;
		this.content = content;
		this.sendPerson = sendPerson;
		this.sendTime = sendTime;
		this.appendDocSet = appendDocSet;
		this.receivePeople = receivePeople;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getSendPerson() {
		return sendPerson;
	}
	public void setSendPerson(String sendPerson) {
		this.sendPerson = sendPerson;
	}
	public String getSendTime() {
		return sendTime;
	}
	public void setSendTime(String sendTime) {
		this.sendTime = sendTime;
	}
	public List<AppendDoc> getAppendDocSet() {
		return appendDocSet;
	}
	public void setAppendDocSet(List<AppendDoc> appendDocSet) {
		this.appendDocSet = appendDocSet;
	}
	public List<ReceivePeople> getReceivePeople() {
		return receivePeople;
	}
	public void setReceivePeople(List<ReceivePeople> receivePeople) {
		this.receivePeople = receivePeople;
	}
	@Override
	public String toString() {
		return "FileTransactionDetail [title=" + title + ", content=" + content
				+ ", sendPerson=" + sendPerson + ", sendTime=" + sendTime
				+ ", appendDocSet=" + appendDocSet + ", receivePeople="
				+ receivePeople + "]";
	}
	
}
