public enum OpinionSeirer {
	intendsComment(1,"nbopinion"),leadershipOppinion(2,"ldpsopinion"),handleComment(3,"foradvice"),quldpsOpinion(4,"qldpsopinion"),zgldOpinion(5,"zgpsopinion"),kyOpinion(6,"kyopinion");
	private int value;
	private String opinionName;
	private OpinionSeirer(int value,String opinionName) {
		value=this.value;
		this.opinionName=opinionName;
	}
	/**
	 * @return the value
	 */
	public int getValue() {
		return value;
	}
	/**
	 * @param value the value to set
	 */
	public void setValue(int value) {
		this.value = value;
	}
	public String getOpinionName() {
		return opinionName;
	}
	public void setOpinionName(String opinionName) {
		this.opinionName = opinionName;
	}
}
